########MOVING ON THE MAP
label finaldestination:
    if pc_area == "creeks" and not creeks_offeredtojoin and quest_easternpath_points >= 5 and quest_missinghunters == 2 and (appearance_charisma+creeks_reputation+(oldhava_friendship/2)+(efren_friendship/2)+(elah_friendship/2)) >= 20 and not elah_quest_easternpath_lumberjacks  and elah_about_nomoreundead != 3 and not elah_locked == day:
        $ custom1 = "Before you cross the bridge, you’re told to wait by {color=#f6d6bd}Elah{/color}. “There’s one more thing, friend."
        jump creeksinvitingpctojointhetribe01
    if pc_area == "whitemarshes" and not whitemarshes_valens_firsttime and not whitemarshes_attacked and not whitemarshes_destroyed:
        $ can_leave = 0
        $ can_rest = 0
        $ can_items = 0
        show areapicture bogcrossroadstowhitemarshes at basicfade
        jump whitemarshes_valens01
    scene empty #part A of...
    scene layoutfull #part B of hididng all images
    stop nature fadeout 2.0
    if pc_area == "eudociahouse":
        $ pc_area = "eudociahouseleaving"
    $ ruinedshelter_duskfoxes_dmgtopc = 0
    $ creeks_reasonstojoin_problemsknown = 0
    if creeks_reasonstojoin_missinghunters:
        $ creeks_reasonstojoin_problemsknown += 1
    if creeks_reasonstojoin_beastattacks:
        $ creeks_reasonstojoin_problemsknown += 1
    if creeks_reasonstojoin_creeksaboutlackingwall:
        $ creeks_reasonstojoin_problemsknown += 1
    if creeks_reasonstojoin_creeksaboutlackingmeat:
        $ creeks_reasonstojoin_problemsknown += 1
    if creeks_reasonstojoin_creeksaboutweakness:
        $ creeks_reasonstojoin_problemsknown += 1
    if creeks_reasonstojoin_hpcknowstruthaboutinbreeding:
        $ creeks_reasonstojoin_problemsknown += 1
    if creeks_reasonstojoin_lackingseeds:
        $ creeks_reasonstojoin_problemsknown += 1
    if creeks_reasonstojoin_lackofmetal:
        $ creeks_reasonstojoin_problemsknown += 1
    if oldhava_about_foggy == 2:
        $ creeks_reasonstojoin_problemsknown += 1
    $ questionpreset = 0
    $ can_leave = 0
    $ can_rest = 0
    $ can_items = 0
    $ can_potions = 0
    $ at_unlock_force = 0
    $ at_unlock_knowledge = 0
    $ at_unlock_spell = 0
    $ at_activate = 0
    $ at = 0
    nvl clear
    if (vines_open_sleep and travel_destination != "vines") and (vines_open_sleep and travel_destination != "whitemarshes"):
        $ vines_open_sleep = 0
    if world_daylength_bogs:
        $ world_daylength_bogs = 0
    label finaldestination_repeat:
        if (pc_area == "eudociahouseleaving" and eudocia_about_roadclearing_cleared and day > eudocia_about_roadclearing_cleared and not eudocia_about_roadclearing_cleared_seen) or (travel_destination == "watchtower" and eudocia_about_roadclearing_cleared and day > eudocia_about_roadclearing_cleared and not eudocia_about_roadclearing_cleared_seen) or (travel_destination == "eudociahouse" and eudocia_about_roadclearing_cleared and day > eudocia_about_roadclearing_cleared and not eudocia_about_roadclearing_cleared_seen):
            $ eudocia_about_roadclearing_cleared_seen = 1
            $ watchtower_roads = 1
            stop music fadeout 4.0
            play nature "audio/ambient/watchtower01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
            show areapicture watchtower_roads at basicfade
            menu:
                'You take a short ride through the roads surrounding the tower. The branches, rocks, plants, and bones are cleared, the trail evened-out, and in many spots you notice large prints left by the boulder-like feet. {color=#f6d6bd}Eudocia{/color} has kept her word.
                '
                'Crossing these hills should be much easier now.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- Crossing these hills should be much easier now.')
                    jump finaldestination_repeat
        if pc_area == "monastery" and aeli_about_bronzerod == 1 and quest_bronzerod == 1 and item_bronzerod and not aeli_about_bronzerod_ignored and not aeli_about_bronzerod_honored:
            show areapicture oldpagostomonastery at basicfade
            stop music fadeout 4.0
            play nature "audio/ambient/mountainroad01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
            menu:
                'As you descend, you think of {color=#f6d6bd}Aeli’s{/color} reaction to the bronze rod. You {i}could{/i} ignore him and take a few minutes to tie it to one of the ledges, just nearby. This place is already high above the woodlands.
                '
                'Just in case, I look behind, then get on the ground.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- Just in case, I look behind, then get on the ground.')
                    if pc_religion == "ordersoftruth":
                        $ pc_faithpoints -= 1
                        $ pc_faithpoints_opportunities += 1
                    $ quarters += 1
                    $ aeli_about_bronzerod_ignored = 1
                    $ item_bronzerod -= 1
                    $ eudocia_bronzerod_installed += 1
                    $ renpy.notify("Journal updated: Bronze Rods")
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='{i}Journal updated: Bronze Rods{/i}')
                    if not item_bronzerod:
                        if eudocia_bronzerod_installed < 4:
                            $ quest_bronzerod_description07 = "I’ve placed fewer than four rods, but I don’t have any of them left. I should inform {color=#f6d6bd}Eudocia{/color}."
                        elif eudocia_bronzerod_installed < eudocia_bronzerod_max and eudocia_bronzerod_rodin_whitemarshes:
                            $ quest_bronzerod_description04 = "I’ve placed some of the rods, but don’t have any of them left. I should collect my reward."
                        elif eudocia_bronzerod_installed >= eudocia_bronzerod_max and eudocia_bronzerod_rodin_whitemarshes:
                            $ quest_bronzerod_description05 = "I’ve placed all of the rods. I should collect my reward."
                        else:
                            $ quest_bronzerod_description03 = "I’ve placed the rods, but not in {color=#f6d6bd}White Marshes{/color}. Let’s hope {color=#f6d6bd}Eudocia{/color} is going to pay anyway."
                    else:
                        if eudocia_bronzerod_installed >= 4:
                            if not quest_bronzerod_description04:
                                $ quest_bronzerod_description04 = "I’ve placed at least four rods. I can return to collect a part of my reward."
                                $ quest_bronzerod_description02 = 0
                    menu:
                        'You seek a spot that isn’t too easily seen from the trail, then make sure it won’t crumble under your weight. After a few minutes, you finish placing the rod - held by a cord and a few heavy rocks. You doubt anyone would stumble upon it.
                        '
                        'I continue my journey.':
                            $ narrator.add_history(kind='nvl', who=narrator.name, what='- I continue my journey.')
                            jump finaldestination_repeat
                'This is the monks’ land, and I should obey their laws. I ride ahead.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- This is the monks’ land, and I should obey their laws. I ride ahead.')
                    $ aeli_about_bronzerod_honored += 1
                    jump finaldestination_repeat
                'This may not be {i}my{/i} order, but I ought to obey the monks’ laws. I ride ahead.' if pc_religion == "ordersoftruth":
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- This may not be {i}my{/i} order, but I ought to obey the monks’ laws. I ride ahead.')
                    $ aeli_about_bronzerod_honored += 1
                    $ pc_faithpoints_opportunities += 1
                    if pc_religion == "ordersoftruth":
                        $ pc_faithpoints -= 1
                    jump finaldestination_repeat
        if (pc_area == "creeks" and quest_easternpath_points >= 6 and not roadtocreeks_faster) or (pc_area == "creeks" and quest_easternpath_points >= 6 and not roadtocreeks_faster):
            show areapicture foggylaketocreeks01 at basicfade
            stop music fadeout 4.0
            play nature "audio/ambient/shortcutwoodenroad01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
            $ roadtocreeks_faster = 1
            $ roadtocreeks_faster_firsttime = 1
            menu:
                'You ride by a patrol from {color=#f6d6bd}Creeks{/color} that’s escorting a cart loaded with timber. They greet you politely, mentioning the growing safety on the roads. As you ride further, you notice the fresh coat of pebbles covering the muddier scraps of the trail, the trimmed shrubs, and the cleared tufts of grass.
                \n\nFrom now on, it should be easier to cross the path between {color=#f6d6bd}Foggy Lake{/color} and the village.
                '
                'Finally, the locals are waking up.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- Finally, the locals are waking up.')
                    jump finaldestination_repeat
                'I’m making a difference.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- I’m making a difference.')
                    jump finaldestination_repeat
    if travel_destination == "peltnorth" and not peltnorth_firsttime:
        if toruinedvillage <= tosoutherncrossroads and ruinedvillage_firsttime:
            show areapicture peltnorthtoruinedvillage at basicfade
            if not renpy.music.get_playing(channel='music') == "<loop 20.0>audio/track_16travelingandprologuedayone.ogg":
                play music "<loop 20.0>audio/track_16travelingandprologuedayone.ogg" fadeout 1.0 fadein 1.0
            menu:
                'The road downhill meanders, slowing you down and forcing you to pay close attention not only to the shadows of the trees, but also to the tight, rocky passages. You can’t help but notice a few caves and fine ambush spots.
                '
                'I keep an eye out for any sign of life.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- I keep an eye out for any sign of life.')
                    jump finaldestinationaftertraveldescription
        else:
            show areapicture southerncrossroadstopeltnorth at basicfade
            stop music fadeout 4.0
            play nature "audio/ambient/ruinedshelter01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
            menu:
                '{color=#f6d6bd}[horsename]{/color} trots with ease, unbothered by the few branches covering the beaten road. The bird songs and distant howls draw your attention to the forest, which gets sparser and brighter. You spot boars, roe deer, and saurians.
                \n\nThe sight of a nearby wolf pack worries you, but once you push your hips forward and your palfrey enters a canter, the beasts don’t even begin a pursuit. The speed alone will protect you from many dangers.
                \n\nYou notice a stone tower, taller than the trees.
                '
                'It must be the inn I’ve heard about.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- It must be the inn I’ve heard about.')
                    jump finaldestinationaftertraveldescription
    if travel_destination == "ruinedvillage" and not ruinedvillage_firsttime:
        if topeltnorth <= tobeholder and peltnorth_firsttime:
            show areapicture peltnorthtoruinedvillage at basicfade
            stop music fadeout 4.0
            play nature "audio/ambient/ruinedshelter01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
            menu:
                'The road uphill meanders, slowing you down and forcing you to pay close attention not only to the shadows of the trees, but also to the tight, rocky passages. You can’t help but notice a few caves and fine ambush spots.
                '
                'I keep an eye out for any sign of life.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- I keep an eye out for any sign of life.')
                    jump finaldestinationaftertraveldescription
        else:
            jump finaldestinationaftertraveldescription
    if travel_destination == "beholder" and not beholder_firsttime:
        if tohowlersdell <= toruinedvillage and howlersdell_firsttime:
            show areapicture beholdertohowlersdell at basicfade
            stop music fadeout 4.0
            play nature "audio/ambient/bogentrance01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
            menu:
                'You slow down. Among the shrubs, a small pack of dragonlings is feasting on the carcass of a gray auroch. One of them, with red feathers on its arms, straightens up, and gives you a curious glance. Its head tilts left and right, forward and back. It screeches at you, but doesn’t move forward, and returns to its meal.
                \n\nYou wonder what would happen if you were on foot. Would they let you be, satisfied with their prey? Or would they run after you? But if so - for what reason? For now, you have enough space to move forward, yet stay safe.
                \n\nThe powerful jaws break the monster’s bones loudly, then bite off and swallow chunks of meat.
                '
                'I keep my distance and look over my shoulder.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- I keep my distance and look over my shoulder.')
                    jump finaldestinationaftertraveldescription
        else:
            show areapicture ruinedvillagetobeholder at basicfade
            stop music fadeout 4.0
            play nature "audio/ambient/huntercabinnight01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
            menu:
                'You enter the wetlands, with their many streams, ponds, and fish. Large birds run and fly between the gray, sparse trees that are covered with moss, mushrooms, and critters. For a human, it may be an unwelcoming and harsh realm, but the overwhelming swarms of insects prove that it’s a realm full of life.
                \n\nThe danger, with teeth and claws, is hidden below the water’s surface.
                '
                'I stay away from the banks.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- I stay away from the banks.')
                    jump finaldestinationaftertraveldescription
    if travel_destination == "druidcave" and not druidcave_firsttime:
        show areapicture beholdertodruidscave at basicfade
        stop music fadeout 4.0
        $ quarters += todruidcave-1
        play nature "audio/ambient/bogroad01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
        menu:
            'The shapes of mountains are cutting through the horizon. The water gets gradually less muddy. The cleaner it gets, the more animals you see.
            \n\nA family of deer blocks the road, resting and enjoying the cold water, which would be deadly for a human stomach. You consider riding around them, through the sparse meadow, but your presence also causes them disquiet. Without much haste, they regroup and run east, into the leafy forest.
            '
            'I travel forward.':
                $ narrator.add_history(kind='nvl', who=narrator.name, what='- I travel forward.')
                $ quarters -= todruidcave-1
                jump finaldestinationaftertraveldescription
    elif travel_destination == "druidcave" and druidcave_firsttime and (druidcave_firsttime_counter+5) < day and not druidcave_deer_secondtime:
        show areapicture beholdertodruidscave at basicfade
        stop music fadeout 4.0
        $ druidcave_deer_secondtime = 1
        $ quarters += todruidcave-1
        play nature "audio/ambient/bogroad01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
        menu:
            'The family of deer you met some days ago has returned to the pond, this time accompanied by more members of their pack. One buck towers over all the others, with antlers as large as your palfrey. It takes a few steps toward you, shaking its head - {i}you better stay where you are{/i}.
            \n\nYou observe their curious, lazy glances. While the adults quench their thirst, the young ones are playing in the tall grasses, or basking in the middle of the road.
            \n\nIt takes quite a few minutes before the large beast orders the others to head back into the woods. Its bellow is like a falling boulder.
            '
            'I wait for a few minutes patiently.':
                $ narrator.add_history(kind='nvl', who=narrator.name, what='- I wait for a few minutes patiently.')
                $ quarters -= todruidcave-1
                $ quarters += 1
                jump finaldestinationaftertraveldescription
            'Stupid beasts.':
                $ narrator.add_history(kind='nvl', who=narrator.name, what='- Stupid beasts.')
                $ quarters -= todruidcave-1
                $ quarters += 1
                jump finaldestinationaftertraveldescription
    if travel_destination == "druidcave" and druidcave_druid_about_plague_travel:
        show areapicture westerncrossroadstooldpagos at basicfade
        stop music fadeout 4.0
        play nature "audio/ambient/bogroad01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
        menu:
            'For the first hour, the elder swings his staff around, like a conjurer in the marketplace, and hums to himself like a drunkard. His walking pace remains stable.
            \n\nAs time goes on, this frenzied state fades out. At first, he returns to using his staff, then gives you his bag. He looks down, hobbling, and finally asks you for help.
            '
            'I get him in the saddle.':
                $ narrator.add_history(kind='nvl', who=narrator.name, what='- I get him in the saddle.')
                $ quarters += 2
                jump finaldestinationaftertraveldescription
    if travel_destination == "howlersdell" and not howlersdell_firsttime:
        if tobeholder <= towesterncrossroads and beholder_firsttime:
            show areapicture beholdertohowlersdell at basicfade
            stop music fadeout 4.0
            play nature "audio/ambient/bogroad01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
            menu:
                'You slow down. Among the shrubs, a small pack of dragonlings is feasting on the carcass of a gray auroch. One of them, with red feathers on its arms, straightens up, and gives you a curious glance. Its head tilts left and right, forward and back. It screeches at you, but doesn’t move forward, and returns to its meal.
                \n\nYou wonder what would happen if you were on foot. Would they let you be, satisfied with their prey? Or would they run after you? But if so - for what reason? For now, you have enough space to move forward, yet stay safe.
                \n\nThe powerful jaws break the monster’s bones loudly, then bite off and swallow chunks of meat.
                '
                'I ride forward, but I keep my distance and look over my shoulder.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- I ride forward, but I keep my distance and look over my shoulder.')
                    jump finaldestinationaftertraveldescription
        elif howlersdell_firsttime_night:
            $ howlersdellforestgardendescription = 1
            show areapicture howlersdelltowesterncrossroads at basicfade
            stop music fadeout 4.0
            play nature "audio/ambient/foragingground01.ogg" fadeout 2.0 fadein 5.0 volume 1.0
            menu:
                'A group of wolves chases after you, but the road is beaten and free of litter, and serves your palfrey well. You notice paths between the fruit-bearing trees on your right, and soon after that, you ride alongside vast fields of green hemp plants, taller than you are, waiting for a scythe or sickle. Then there are broad beans, much shorter, but healthy, almost ready for the second harvest of the summer.
                \n\nThe path ends with a massive wooden gate. The stone wall, separating you from the glow of lights behind it, isn’t as tall as the roofs, but you can’t imagine climbing to its top. There are pretty much no cracks, no missing bricks, a sign of many hours of labor, especially considering how massive the settlement is, as wide as a city district.
                \n\nThe birds and monkeys are now just a distant echo, while the beasts that were pursuing you stay in place, too afraid to get any closer.
                '
                'I ride forward, ignoring my wounds, before they resume the pursuit.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- I ride forward, ignoring my wounds, before they resume the pursuit.')
                    jump finaldestinationaftertraveldescription
                '*sigh* Let’s get this over with.':
                    $ narrator.add_history(kind='nvl', who=narrator.name, what='- *sigh* Let’s get this over with.')
                    jump finaldestinationaftertraveldescription
        else:
            $ howlersdellforestgardendescription = 1
